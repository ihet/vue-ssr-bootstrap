{
    "presets":[
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "targets": "> 0.25%, not dead",
                "corejs": { "version": 3, "proposals": true },
                "modules": false
            }
      ],
      [
        "@babel/preset-typescript",
        {
            "allExtensions":true,
            "allowNamespaces":true
        }
      ]
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-transform-runtime"
    ]
}